PROCEDURE validardatos
LOCAL loResult, lcSql

loResult = CREATEOBJECT("odbc_result")
lcSql = ""

IF !DODEFAULT() THEN
	RETURN .F.
ENDIF

lcSql = "SELECT nroCuit FROM clientes " ;
	+ "WHERE nrocuit = '" + ALLTRIM(thisform.pgf.page2.cnt_fields.txtNroCuit.Value) + "' ";
	+ "	AND idCliente != " + ALLTRIM(STR(cur_Tempo.idCliente)) ;
	+ " AND fecBaja IS NULL "

loResult.ActiveConnection = goConn.ActiveConnection
loResult.cursor_name = "cur_cliente"
loResult.OpenQuery(lcSql)

SELECT cur_cliente
IF RECCOUNT() > 0
	IF MESSAGEBOX("El número de Cuit ingresado ya se encuentra cargado a otro cliente, desea grabarlo igualmente?",4+32,thisform.Caption) != 6
		loResult.close_query()
		Thisform.msgerror = "CUIT Duplicado"
		RETURN .F.
	ENDIF 
ENDIF 

* Verifico el CUIT
IF Thisform.pgf.page2.cnt_fields.sel_tipoDoc.valcpoid = 1 .OR. Thisform.pgf.page2.cnt_fields.sel_tipoDoc.valcpoid = 6 THEN
	IF !ValidarCUIT(ALLTRIM(Thisform.pgf.page2.cnt_fields.txtNroCuit.Value)) THEN
		Thisform.msgerror = "El CUIT no es válido, por favor verifique el mismo y corrijalo"
		&& MESSAGEBOX("El CUIT no es válido, por favor verifique el mismo y corrijalo", 0+48, Thisform.Caption)
		Thisform.pgf.page2.cnt_fields.txtNroCuit.SetFocus()
		RETURN .F.
	ENDIF
ENDIF

RETURN .T.
ENDPROC
PROCEDURE recuperardatos
LOCAL loLoc, loPcia, loPais, lcSql

DODEFAULT()

lcSql = "select * from localidad where idlocalid = " + ALLTRIM(STR(thisform.pgf.page2.cnt_fields.sel_Localid.valcpoid))
loLoc = CREATEOBJECT("odbc_Result")
loLoc.ActiveConnection = goConn.ActiveConnection
loLoc.Cursor_Name = "cur_Localidad"
loLoc.OpenQuery(lcSql)

SELECT cur_Localidad
lcSql = "select * from provincias where idprovin = " + ALLTRIM(STR(cur_Localidad.idProvin))
loPcia = CREATEOBJECT("odbc_Result")
loPcia.ActiveConnection = goConn.ActiveConnection
loPcia.Cursor_Name = "cur_pcia"
loPcia.OpenQuery(lcSql)

SELECT cur_pcia
thisform.pgf.page2.cnt_fields.txtProvincia.Value = cur_pcia.descripcio

SELECT cur_Localidad
lcSql = "select * from paises where idpais = " + ALLTRIM(STR(cur_Localidad.idpais))
loPais = CREATEOBJECT("odbc_Result")
loPais.ActiveConnection = goConn.ActiveConnection
loPais.Cursor_Name = "cur_pais"
loPais.OpenQuery(lcSql)

SELECT cur_pais
thisform.pgf.page2.cnt_fields.txtpais.Value = cur_pais.descripcio

loPais.close_query()
loPcia.close_query()
loLoc.close_query()
ENDPROC
PROCEDURE validar_eliminar
LOCAL loData, lcSql

lcSql = "select idcliente from ventascab where idcliente = " + ALLTRIM(STR(cur_tempo.idcliente))
loData = CREATEOBJECT("odbc_result")
